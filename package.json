{
  "name": "v2fun",
  "version": "1.8.4",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "android": "cd android && ./gradlew clean && react-native run-android",
    "android:release": "yarn bundle:android && cd android && ./gradlew clean assembleRelease",
    "ios": "react-native run-ios",
    "ios:release": "yarn bundle:ios && cd ios && xcodebuild -workspace v2fun.xcworkspace -scheme v2fun -configuration Release -destination generic/platform=iOS -archivePath build/v2fun.xcarchive archive",
    "start": "react-native start --client-logs",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "eslint .",
    "test": "jest",
    "postinstall": "patch-package",
    "ios:ipa": "cd ios && xcodebuild -exportArchive -archivePath build/v2fun.xcarchive -exportPath build/ipa -exportOptionsPlist exportOptions.plist",
    "ios:clean": "cd ios && xcodebuild clean -workspace v2fun.xcworkspace -scheme v2fun"
  },
